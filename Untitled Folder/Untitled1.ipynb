   "source": [
    "import string\n",
    "\n",
    "tmp = string.digits+string.ascii_lowercase\n",
    "def convert(num, base) :\n",
    "    q, r = divmod(num, base)\n",
    "    if q == 0 :\n",
    "        return tmp[r] \n",
    "    else :\n",
    "        return convert(q, base) + tmp[r]"
   ]
  },
